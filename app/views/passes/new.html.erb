pass登録ページ

<br>
以下の項目を埋めてください

<%= form_for (@pass)do |f| %>
<%= f.label :title,:タイトル %>
<%= f.text_field :title %>
<br>
<%= f.label :image,:写真 %>
<%= f.file_field :image %>
<br>
<%= f.label :content,:内容 %>
<%= f.text_area :content %>
<br>
<%= f.label :price,:値段 %>
<%= f.number_field :price %>
<br>
<%= f.label :time,:所要時間 %>
<%= f.number_field :time %>
<br>
<%= f.label :conditions,:条件 %>
<%= f.text_area :conditions %>
<br>
<%= f.submit :送信 %>

<% end %>
